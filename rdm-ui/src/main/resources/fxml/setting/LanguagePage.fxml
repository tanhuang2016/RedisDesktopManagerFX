<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="root" prefHeight="600" prefWidth="600" styleClass="root" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xyz.hashdog.rdm.ui.controller.setting.AdvancedPageController">
   <children>
      <VBox prefHeight="600.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="TOP_CENTER" spacing="60.0">
               <children>
                   <HBox alignment="CENTER_LEFT" spacing="6.0" VBox.vgrow="ALWAYS">
                       <children>
                               <Label text="语言" />
                        <ComboBox HBox.hgrow="ALWAYS" />
                       </children>
                   </HBox>
               </children>
            </VBox>
             <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="591.0" spacing="10.0">
                 <children>
                     <Button fx:id="ok" disable="true" mnemonicParsing="false" onAction="#ok" text="确定" />
                     <Button fx:id="reset" mnemonicParsing="false" onAction="#reset" text="系统" />
                 </children>
             </HBox>
         </children>
      </VBox>
   </children>

    <stylesheets>
        <URL value="@../../css/global.css" />
    </stylesheets>
</AnchorPane>
